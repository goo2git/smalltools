<P>This code is contained in a callback function that is called every =
time the=20
<CODE>waveIn</CODE> functions return with updated audio signal data. The =
code=20
that actually performs the FFT looks like:</P><PRE><SPAN =
class=3Dcode-keyword>void</SPAN> fft_double (<SPAN =
class=3Dcode-keyword>unsigned</SPAN> <SPAN =
class=3Dcode-keyword>int</SPAN> p_nSamples, <SPAN =
class=3Dcode-keyword>bool</SPAN> p_bInverseTransform,=20
    <SPAN class=3Dcode-keyword>double</SPAN> *p_lpRealIn, <SPAN =
class=3Dcode-keyword>double</SPAN> *p_lpImagIn,=20
    <SPAN class=3Dcode-keyword>double</SPAN> *p_lpRealOut, <SPAN =
class=3Dcode-keyword>double</SPAN> *p_lpImagOut)
{

    <SPAN class=3Dcode-keyword>if</SPAN>(!p_lpRealIn || !p_lpRealOut || =
!p_lpImagOut) <SPAN class=3Dcode-keyword>return</SPAN>;


    <SPAN class=3Dcode-keyword>unsigned</SPAN> <SPAN =
class=3Dcode-keyword>int</SPAN> NumBits;
    <SPAN class=3Dcode-keyword>unsigned</SPAN> <SPAN =
class=3Dcode-keyword>int</SPAN> i, j, k, n;
    <SPAN class=3Dcode-keyword>unsigned</SPAN> <SPAN =
class=3Dcode-keyword>int</SPAN> BlockSize, BlockEnd;

    <SPAN class=3Dcode-keyword>double</SPAN> angle_numerator =3D <SPAN =
class=3Dcode-digit>2</SPAN>.<SPAN class=3Dcode-digit>0</SPAN> * PI;
    <SPAN class=3Dcode-keyword>double</SPAN> tr, ti;

    <SPAN class=3Dcode-keyword>if</SPAN>( !IsPowerOfTwo(p_nSamples) )
    {
        <SPAN class=3Dcode-keyword>return</SPAN>;
    }

    <SPAN class=3Dcode-keyword>if</SPAN>( p_bInverseTransform ) =
angle_numerator =3D -angle_numerator;

    NumBits =3D NumberOfBitsNeeded ( p_nSamples );


    <SPAN class=3Dcode-keyword>for</SPAN>( i=3D0; i <SPAN =
class=3Dcode-keyword>&lt;</SPAN> p_nSamples; i++ )
    {
        j =3D ReverseBits ( i, NumBits );
        p_lpRealOut[j] =3D p_lpRealIn[i];
        p_lpImagOut[j] =3D (p_lpImagIn =3D=3D NULL) ? <SPAN =
class=3Dcode-digit>0</SPAN>.<SPAN class=3Dcode-digit>0</SPAN> : =
p_lpImagIn[i];
    }


    BlockEnd =3D <SPAN class=3Dcode-digit>1</SPAN>;
    <SPAN class=3Dcode-keyword>for</SPAN>( BlockSize =3D <SPAN =
class=3Dcode-digit>2</SPAN>; BlockSize <SPAN =
class=3Dcode-keyword>&lt;</SPAN>=3D p_nSamples; BlockSize <SPAN =
class=3Dcode-keyword>&lt;</SPAN><SPAN =
class=3Dcode-keyword>&lt;</SPAN>=3D <SPAN class=3Dcode-digit>1</SPAN> )
    {
        <SPAN class=3Dcode-keyword>double</SPAN> delta_angle =3D =
angle_numerator / (<SPAN class=3Dcode-keyword>double</SPAN>)BlockSize;
        <SPAN class=3Dcode-keyword>double</SPAN> sm2 =3D sin ( -<SPAN =
class=3Dcode-digit>2</SPAN> * delta_angle );
        <SPAN class=3Dcode-keyword>double</SPAN> sm1 =3D sin ( =
-delta_angle );
        <SPAN class=3Dcode-keyword>double</SPAN> cm2 =3D cos ( -<SPAN =
class=3Dcode-digit>2</SPAN> * delta_angle );
        <SPAN class=3Dcode-keyword>double</SPAN> cm1 =3D cos ( =
-delta_angle );
        <SPAN class=3Dcode-keyword>double</SPAN> w =3D <SPAN =
class=3Dcode-digit>2</SPAN> * cm1;
        <SPAN class=3Dcode-keyword>double</SPAN> ar[<SPAN =
class=3Dcode-digit>3</SPAN>], ai[<SPAN class=3Dcode-digit>3</SPAN>];

        <SPAN class=3Dcode-keyword>for</SPAN>( i=3D0; i <SPAN =
class=3Dcode-keyword>&lt;</SPAN> p_nSamples; i +=3D BlockSize )
        {

            ar[<SPAN class=3Dcode-digit>2</SPAN>] =3D cm2;
            ar[<SPAN class=3Dcode-digit>1</SPAN>] =3D cm1;

            ai[<SPAN class=3Dcode-digit>2</SPAN>] =3D sm2;
            ai[<SPAN class=3Dcode-digit>1</SPAN>] =3D sm1;

            <SPAN class=3Dcode-keyword>for</SPAN> ( j=3Di, n=3D0; n =
<SPAN class=3Dcode-keyword>&lt;</SPAN> BlockEnd; j++, n++ )
            {

                ar[<SPAN class=3Dcode-digit>0</SPAN>] =3D w*ar[<SPAN =
class=3Dcode-digit>1</SPAN>] - ar[<SPAN class=3Dcode-digit>2</SPAN>];
                ar[<SPAN class=3Dcode-digit>2</SPAN>] =3D ar[<SPAN =
class=3Dcode-digit>1</SPAN>];
                ar[<SPAN class=3Dcode-digit>1</SPAN>] =3D ar[<SPAN =
class=3Dcode-digit>0</SPAN>];

                ai[<SPAN class=3Dcode-digit>0</SPAN>] =3D w*ai[<SPAN =
class=3Dcode-digit>1</SPAN>] - ai[<SPAN class=3Dcode-digit>2</SPAN>];
                ai[<SPAN class=3Dcode-digit>2</SPAN>] =3D ai[<SPAN =
class=3Dcode-digit>1</SPAN>];
                ai[<SPAN class=3Dcode-digit>1</SPAN>] =3D ai[<SPAN =
class=3Dcode-digit>0</SPAN>];

                k =3D j + BlockEnd;
                tr =3D ar[<SPAN =
class=3Dcode-digit>0</SPAN>]*p_lpRealOut[k] - ai[<SPAN =
class=3Dcode-digit>0</SPAN>]*p_lpImagOut[k];
                ti =3D ar[<SPAN =
class=3Dcode-digit>0</SPAN>]*p_lpImagOut[k] + ai[<SPAN =
class=3Dcode-digit>0</SPAN>]*p_lpRealOut[k];

                p_lpRealOut[k] =3D p_lpRealOut[j] - tr;
                p_lpImagOut[k] =3D p_lpImagOut[j] - ti;

                p_lpRealOut[j] +=3D tr;
                p_lpImagOut[j] +=3D ti;

            }
        }

        BlockEnd =3D BlockSize;

    }


    <SPAN class=3Dcode-keyword>if</SPAN>( p_bInverseTransform )
    {
        <SPAN class=3Dcode-keyword>double</SPAN> denom =3D (<SPAN =
class=3Dcode-keyword>double</SPAN>)p_nSamples;

        <SPAN class=3Dcode-keyword>for</SPAN> ( i=3D0; i <SPAN =
class=3Dcode-keyword>&lt;</SPAN> p_nSamples; i++ )
        {
            p_lpRealOut[i] /=3D denom;
            p_lpImagOut[i] /=3D denom;
        }
    }

}</PRE>
<P>And it requires the following supporting functions:</P><PRE><SPAN =
class=3Dcode-SummaryComment>///</SPAN><SPAN =
class=3Dcode-comment>////////////////////////////////////////////////////=
////
</SPAN><SPAN class=3Dcode-comment>//</SPAN><SPAN class=3Dcode-comment> =
check is a number is a power of 2
</SPAN><SPAN class=3Dcode-SummaryComment>///</SPAN><SPAN =
class=3Dcode-comment>////////////////////////////////////////////////////=
////
</SPAN>
<SPAN class=3Dcode-keyword>bool</SPAN> IsPowerOfTwo( <SPAN =
class=3Dcode-keyword>unsigned</SPAN> <SPAN =
class=3Dcode-keyword>int</SPAN> p_nX )
{

    <SPAN class=3Dcode-keyword>if</SPAN>( p_nX <SPAN =
class=3Dcode-keyword>&lt;</SPAN> <SPAN class=3Dcode-digit>2</SPAN> ) =
<SPAN class=3Dcode-keyword>return</SPAN> <SPAN =
class=3Dcode-keyword>false</SPAN>;

    <SPAN class=3Dcode-keyword>if</SPAN>( p_nX &amp; (p_nX-<SPAN =
