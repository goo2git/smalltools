</SPAN>        TRACE(<SPAN class=3Dcode-string>"</SPAN><SPAN =
class=3Dcode-string>Average for Bar#%d is %d\n"</SPAN>,nCur++,nAvg);
        i+=3DnBars-<SPAN class=3Dcode-digit>1</SPAN>;
    }
    DataHolder* pDataHolder =3D (DataHolder*)lpData;
    <SPAN class=3Dcode-comment>//</SPAN><SPAN class=3Dcode-comment> Draw =
