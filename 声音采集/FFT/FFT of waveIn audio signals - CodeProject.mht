class=3Dcode-keyword>int</SPAN> i, rev;

    <SPAN class=3Dcode-keyword>for</SPAN>(i=3Drev=3D0; i <SPAN =
class=3Dcode-keyword>&lt;</SPAN> p_nBits; i++)
