<SPAN class=3Dcode-keyword>unsigned</SPAN> <SPAN =
class=3Dcode-keyword>int</SPAN> ReverseBits(<SPAN =
class=3Dcode-keyword>unsigned</SPAN> <SPAN =
class=3Dcode-keyword>int</SPAN> p_nIndex, <SPAN =
class=3Dcode-keyword>unsigned</SPAN> <SPAN =
class=3Dcode-keyword>int</SPAN> p_nBits)
{

