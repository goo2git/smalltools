class=3Dcode-keyword>double</SPAN>[] _waveLeft;
    <SPAN class=3Dcode-keyword>private</SPAN> <SPAN =
class=3Dcode-keyword>double</SPAN>[] _waveRight;
    <SPAN class=3Dcode-keyword>private</SPAN> <SPAN =
class=3Dcode-keyword>double</SPAN>[] _fftLeft;
    <SPAN class=3Dcode-keyword>private</SPAN> <SPAN =
class=3Dcode-keyword>double</SPAN>[] _fftRight;
    <SPAN class=3Dcode-keyword>private</SPAN> SignalGenerator =
_signalGenerator;
    <SPAN class=3Dcode-keyword>private</SPAN> <SPAN =
class=3Dcode-keyword>bool</SPAN> _isTest =3D <SPAN =
class=3Dcode-keyword>false</SPAN>;
    <SPAN class=3Dcode-keyword>public</SPAN> AudioFrame(<SPAN =
class=3Dcode-keyword>bool</SPAN> isTest)
    {
        _isTest =3D isTest;
    }

    <SPAN class=3Dcode-SummaryComment>///</SPAN><SPAN =
class=3Dcode-comment> <SPAN class=3Dcode-SummaryComment>&lt;</SPAN><SPAN =
class=3Dcode-SummaryComment>summary</SPAN><SPAN =
class=3Dcode-SummaryComment>&gt;</SPAN>
</SPAN>    <SPAN class=3Dcode-SummaryComment>///</SPAN><SPAN =
class=3Dcode-comment> Process 16 bit sample
</SPAN>    <SPAN class=3Dcode-SummaryComment>///</SPAN><SPAN =
class=3Dcode-comment> <SPAN class=3Dcode-SummaryComment>&lt;</SPAN><SPAN =
class=3Dcode-SummaryComment>/</SPAN><SPAN =
class=3Dcode-SummaryComment>summary</SPAN><SPAN =
class=3Dcode-SummaryComment>&gt;</SPAN>
</SPAN>    <SPAN class=3Dcode-SummaryComment>///</SPAN><SPAN =
class=3Dcode-comment> <SPAN class=3Dcode-SummaryComment>&lt;</SPAN><SPAN =
class=3Dcode-SummaryComment>param</SPAN> <SPAN =
class=3Dcode-SummaryComment>name=3D"wave"</SPAN><SPAN =
class=3Dcode-SummaryComment>&gt;</SPAN><SPAN =
class=3Dcode-SummaryComment>&lt;</SPAN><SPAN =
